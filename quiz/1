print("who is the father of computer science")
x=input("1.charles babbage 2.blaise pascal 3.john napier 4.newton")

x=str(x)



if(x=="charles babbage"):
  print("right")
elif(x=="blaise pascal"):
  print("wrong")
elif(x=="john napier"):
  print("wrong")
elif(x=="newton"):
  print("wrong")

if(x=="charles babbage"):
